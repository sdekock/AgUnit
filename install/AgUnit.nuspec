    <copyright>Copyright 2013 Steven de Kock</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="ReSharper" version="8.0" />
    </dependencies>
    <tags>resharper unittest silverlight statlight</tags>
